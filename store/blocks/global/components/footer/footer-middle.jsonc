{
  "flex-layout.row#global__footer--middle": {
    "title": "global__footer--middle",
    "children": [
      "flex-layout.col#footer__middle--col-one",
      "flex-layout.col#footer__middle--col-two",
      "flex-layout.col#footer__middle--col-three",
      "flex-layout.col#footer__middle--col-four"
    ], 
    "props": {"blockClass": "desktop__footer--middle"}
  }, 
  "flex-layout.col#footer__middle--col-one": {
    "title": "footer__middle--column-one",
    "children": [
      "logo#desktop__header--logo",
      "rich-text#footer__middle--follow-us",
      "flex-layout.row#footer__middle--social-networks", 
      "rich-text#footer__middle--store", 
      "flex-layout.row#footer__middle--select-store"
    ],
    "props": {"blockClass":["footer__middle--col-one", "mobile__footer--middle-col-one"]}
  },
    "rich-text#footer__middle--follow-us": {
      "title": "Title-follow-us", 
      "props": {
        "blockClass": "footer__middle--col-one-text",
        "text": "Siguenos:"
      }
    },
    "flex-layout.row#footer__middle--social-networks": {
      "title": "footer__middle--social-networks",
      "children": [
        "image#social__networks--facebook", 
        "image#social__networks--instagram", 
        "image#social__networks--youtube", 
        "image#social__networks--tiktok"
      ], 
      "props": {"blockClass": "footer__middle--social-networks", "colSizing": "auto"}
    },
      "image#social__networks--facebook": {
        "title": "image-facebook", 
        "props": {
          "src": "assets/img/facebook.svg", 
          "blockClass": "image-social-networks",
          "link": {
            "url": "https://www.facebook.com/kanupets/", 
            "newTab": true      
          }
        }
      },
      "image#social__networks--instagram": {
        "title": "image-instagram", 
        "props": {
          "src": "assets/img/instagram.svg", 
          "blockClass": "image-social-networks",
          "link": {
            "url": "https://www.instagram.com/kanumascotas/", 
            "newTab": true      
          }
        }
      },
      "image#social__networks--youtube" : {
        "title": "image-youtube", 
        "props": {
          "src": "assets/img/youtube.png", 
          "blockClass": "image-social-networks",
          "link": {
            "url": "https://www.youtube.com/channel/UCAkGoOl6XDSK7NbguP60U_g", 
            "newTab": true      
          }
        }
      },
      "image#social__networks--tiktok" : {
        "title": "image-tiktok", 
        "props": {
          "src": "assets/img/tiktok.svg", 
          "blockClass": "image-social-networks",
          "link": {
            "url": "https://www.tiktok.com/@kanupets", 
            "newTab": true      
          }
        }
      },
    "rich-text#footer__middle--store": {
      "title": "", 
      "props": {
        "blockClass": "footer__middle--col-one-text",
        "text": "Encuentra tu local"
      }
    }, 
    "flex-layout.row#footer__middle--select-store": {
      "title": "",
      "children": ["disclosure-layout#footer__middle--select-store"], 
      "props": {
        "blockClass": "footer__middle--select-store"
      }
    },
      "disclosure-layout#footer__middle--select-store": {
        "children": ["disclosure-trigger#select-store", "disclosure-content#select-store"]
      }, 
      "disclosure-trigger#select-store": {
        "children": ["rich-text#select-store"], 
        "props": {"blockClass": "trigger__select--store"}
      }, 
      "rich-text#select-store": {
        "props": {
          "blockClass": "select-store-title",
          "text": "Selecciona tu tienda"
        }
      }, 
      "disclosure-content#select-store": {
        "children": ["menu#select-store"],
        "props": {"blockClass": "content__select--store"}
      }, 
      "menu#select-store": {
        "children": [
          "link#category__links--interest-item-one",
          "link#category__links--interest-item-two",
          "link#category__links--interest-item-three",
          "link#category__links--interest-item-four",
          "link#category__links--interest-item-five"
        ], 
        "props": {
          "orientation": "vertical"
        }
      }, 
  "flex-layout.col#footer__middle--col-two": {
    "children": ["menu#about-us"],
    "props": {"blockClass":"footer__middle--menu"}
  },
