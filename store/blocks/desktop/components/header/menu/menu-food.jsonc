        "flex-layout.row#category__typefood--title": {
          "title": "structure-category-type-food-title", 
          "children": [
            "link#category__typefood--title",
            "icon#desktop__header--submenu"
          ], 
          "props": {
            "colSizing": "auto", 
            "blockClass": "category__typefood--title"
          }
        }, 
          "link#category__typefood--title": {
            "title": "title-type-food", 
            "props": {
              "blockClass": "category__subtitles", 
              "href": "/",
              "label": "Tipo de alimento"
            }
          },
          "icon#desktop__header--submenu": {
            "title": "icon-header-submenu", 
            "props": {
              "id": "submenu-icon",
              "activeClassName": "submenuIcon"
            }
