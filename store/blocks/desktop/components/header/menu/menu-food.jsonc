    },
    "menu#category__food":{
      "title": "header-menu-category-food", 
      "children": [
        "flex-layout.row#menu__category--typefood", 
        "flex-layout.row#menu__category--medicatedfood",
        "flex-layout.row#menu__category--kindpet",
        "flex-layout.row#menu__category--all"
      ], 
      "props": {
        "blockClass": "block-container", 
        "orientation": "horizontal"
      }
    },
/*************************** Category type of food *******************************/
      "flex-layout.row#menu__category--typefood": {
        "title": "menu__category--typefood",
        "children": ["flex-layout.col#category__typefood--container--items"], 
        "props": {"blockClass":"main-blocks"}
