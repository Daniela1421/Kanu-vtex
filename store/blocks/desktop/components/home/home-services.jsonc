        "props": {
          "src": "assets/img/home__section--services-delivery.png", 
          "maxWidth": 40,
          "blockClass": ["section__services2", "section-services-part2"]
        }
      }, 
      "rich-text#section__services2-msg4" : {
        "title": "info-delivery-time", 
        "props": {
          "blockClass": "section__services2-text",
          "text": "**Entrega en 3 días hábiles**\n aprox. En temporada de promociones\n puede variar",
          "textAlignment": "CENTER"
        }
      }
}